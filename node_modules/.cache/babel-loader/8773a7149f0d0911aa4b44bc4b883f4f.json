console.log(config);\n              console.log(config.dataPointIndex);\n              var id = ''\n              console.log(this.list);\n              for (let r = 0; r < this.list.length; r++) {\n                console.log(r);\n                if (config.dataPointIndex == r) {\n                  id = this.list[r].id\n                }\n              }\n              console.log(id);\n              window.open(\"detailtypetwo/\" + id, \"_blank\");\n            },\n            // click: function (event, chartContext, config) {\n            //     window.open(\"detaillevel/\" + config.xaxis.categories[config.dataPointIndex], \"_blank\");\n            // },\n          },\n        },\n        title: {\n          text: \"กราฟแสดงจำนวนผลิตภัณฑ์ที่ไม่ผ่านการตรวจสอบตามเงื่อนไข\",\n        },\n        style: {\n          fontSize: \"9px\",\n          fontWeight: \"bold\",\n          fontFamily: \"Niramit\",\n        },\n      };\n    },\n  },\n  computed: {\n    currentUser() {\n      return this.$store.state.auth.user;\n    },\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n</style>\n"]}]}